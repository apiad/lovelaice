{
  "name": "lovelaice",
  "displayName": "Lovelaice",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lovelaice.Lovelaice: General Query",
        "title": "Lovelaice: General Query"
      }
    ],
    "keybindings": [
      {
        "command": "lovelaice.Lovelaice: General Query",
        "key": "CTRL+SHIFT+L"
      }
    ],
    "configuration": {
      "title": "Lovelaice",
      "properties": {
        "lovelaice.mistral-api-key": {
          "description": "An API key for Mistral.ai",
          "type": "string",
          "default": null
        }
      }
    }
  },
  "activationEvents": [
    "onCommand:lovelaice.Lovelaice: General Query"
  ],
  "dependencies": {
    "ws": "^8.4.0"
  },
  "version": "0.1.8",
  "engines": {
    "vscode": "^1.58.0"
  },
  "publisher": "apiad"
}